{
    "compilerOptions": {
      "module": "esnext",
      "target": "es2017",
      "strict": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "svelte/*": ["./node_modules/svelte/internal"]
